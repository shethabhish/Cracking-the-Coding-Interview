   "source": [
    "def recursive_multiply(num1,num2):\n",
    "    if num1 < num2:\n",
    "        return recursive_multiply(num2,num1)\n",
    "    elif num2 != 0:\n",
    "        return (num1+recursive_multiply(num1,num2-1))\n",
    "    else:\n",
    "        return 0"
   ]
