   "source": [
    "def word_freq(sentence):\n",
    "    sentence = list(sentence.split())\n",
    "    freq = {}\n",
    "    for word in sentence:\n",
    "        if word not in freq:\n",
    "            freq[word] = 0\n",
    "        freq[word] += 1\n",
    "    return freq"
   ]
  },
